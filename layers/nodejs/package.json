    "express-graphql": "0.12.0",
    "fs-extra": "^11.1.1",
    "graphql": "^16.8.1",
    "graphql-compose": "^9.0.10",
    "graphql-query-complexity": "^0.12.0",
    "graphql-subscriptions": "^2.0.0",
