{
  "name": "@jeff-tian/gatsby-source-yuque",
  "publishConfig": {
    "access": "public"
  },
  "description": "Source plugin for pulling data into Gatsby from [语雀](https://www.yuque.com) API.",
  "version": "1.4.16",
  "author": "Raincal <cyj94228@gmail.com>",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/Raincal/gatsby-yuque/tree/master/packages/gatsby-source-yuque#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raincal/gatsby-yuque/tree/master/packages/gatsby-source-yuque"
  },
  "bugs": {
    "url": "https://github.com/Raincal/gatsby-yuque/tree/master/packages/gatsby-source-yuque/issues"
  },
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__,**/node_modules",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__,**/node_modules",
    "prepublishOnly": "npm run test",
    "test": "mocha --timeout 3000 __tests__/**/*.test.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "axios": "^0.19.2",
    "dayjs": "^1.8.22",
    "debug": "^4.1.1",
    "gray-matter": "^4.0.2",
    "html-entities": "^1.2.1",
    "queue": "^6.0.1",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "babel-preset-gatsby-package": "^0.2.17",
    "cross-env": "^7.0.2"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "yuque"
  ],
  "peerDependencies": {
    "gatsby-source-filesystem": "^2.1.53"
  },
  "gitHead": "75915c3977cf548194efe1566e59599b1e13e7d9"
}
