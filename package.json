{
  "name": "serverless-space",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "node index.js",
    "test": "jest",
    "build": "nest build",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "offline": "yarn build && serverless offline",
    "deploy": "serverless deploy",
    "pre-deploy": "cp package.json layers/nodejs/package.json && cd layers/nodejs && npm i --production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jeff-Tian/serverless-space.git"
