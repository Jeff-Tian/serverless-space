{
  "name": "serverless-space",
  "version": "1.1.0",
  "description": "万能 BFF：这是我利用 AWS 免费资源构建一个 SFF 的实验场地，用于验证一些 Serverless 的想法。AWS 一共有 200 多个服务，目前这里使用的仅仅是一些 VPC 之外的服务。",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "node index.js",
    "test": "mocha 'test/**/*.test.js' --recursive && jest",
    "build": "nest build&&cp \"schema.gql\" \"dist/schema.gql\"&&cp \"src/babel-service/babel.min.js\" \"dist/babel-service/babel.min.js\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json --detectOpenHandles",
    "offline": "yarn build && cross-env NODE_ENV=local serverless offline",
    "deploy": "serverless deploy --verbose&&(./smoke || sh smoke.sh)",
    "deploy-from-local": "AWS_SHARED_CREDENTIALS_FILE=~/.aws/config AWS_PROFILE=lambda-doc-rotary serverless deploy",
    "pre-deploy": "cd layers/nodejs && npm i --omit=dev --legacy-peer-deps&&cd ../../gatsby-plugins/nodejs&&(npm i --omit=dev --legacy-peer-deps && npm v gatsby dist.tarball | xargs curl | tar -xz&&(mv package node_modules/gatsby || echo 'mv skipped'))",
    "aws-console": "open https://console.aws.amazon.com/lambda/home?region=us-east-1#/functions",
    "aws-console:windows": "start https://console.aws.amazon.com/lambda/home?region=us-east-1#/functions",
    "studio:windows": "start https://jqp5j170i6.execute-api.us-east-1.amazonaws.com/dev/gatsby/graphql",
    "ci-check": "yarn test&&yarn test:e2e&&yarn build",
    "schema": "yarn offline",
    "mocha": "mocha --timeout 10000",
    "postinstall": "node postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jeff-Tian/serverless-space.git"
  },
  "author": "Jeff Tian <jeff.tian@outlook.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jeff-Tian/serverless-space/issues"
  },
  "homepage": "https://github.com/Jeff-Tian/serverless-space#readme",
  "dependencies": {
    "@apollo/gateway": "2.5.1",
    "@apollo/server": "^4.9.0",
    "@apollo/subgraph": "2.5.1",
    "@aws-sdk/client-dynamodb": "^3.421.0",
    "@aws-sdk/client-sqs": "^3.421.0",
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "@hapi/hoek": "^11.0.2",
    "@hapi/joi": "15.1.1",
    "@jeff-tian/gatsby-source-yuque": "3.0.3",
    "@nestjs/apollo": "11.0.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "10.2.7",
    "@nestjs/core": "10.2.7",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/graphql": "11.0.0",
    "@nestjs/platform-express": "10.2.7",
    "@nestjs/swagger": "^7.1.13",
    "@vendia/serverless-express": "4.10.4",
    "apollo-server-cache-redis": "^3.2.0",
    "apollo-server-express": "^3.5.0",
    "axios": "^1.6.0",
    "better-queue": "^3.8.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "configstore": "^6.0.0",
    "dotenv": "^16.3.1",
    "execa": "^7.2.0",
    "express": "4.18.2",
    "express-graphql": "0.12.0",
    "extract-files": "^13.0.0",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "gatsby": "5.11.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-source-graphql": "5.11.0",
    "graphql": "^16.7.1",
    "graphql-compose": "^9.0.10",
    "graphql-query-complexity": "^0.12.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "9.0.0",
    "hoek": "^6.1.3",
    "ioredis": "^5.3.2",
    "is-url": "^1.2.4",
    "jest-diff": "^29.6.2",
    "js-yaml": "^4.1.0",
    "lock": "^1.1.0",
    "markdown-it": "^13.0.2",
    "mime": "3.0.0",
    "mitt": "^3.0.1",
    "prettier": "^3.0.0",
    "ramda": "^0.29.0",
    "react": "^18.2.0",
    "react-reconciler": "^0.29.0",
