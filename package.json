    "better-queue": "^3.8.10",
    "class-validator": "^0.13.1",
    "configstore": "^6.0.0",
    "execa": "^5.1.1",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "fs-extra": "^10.0.0",
    "gatsby-core-utils": "^2.14.0",
    "gatsby-source-filesystem": "^3.14.0",
    "graphql": "14.7.0",
    "graphql-compose": "^9.0.3",
