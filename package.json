  },
  "homepage": "https://github.com/Jeff-Tian/serverless-space#readme",
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@hapi/hoek": "^9.2.0",
    "@hapi/joi": "15.1.1",
    "@jeff-tian/gatsby-source-yuque": "^1.4.17",
    "@mdx-js/mdx": "^1.6.22",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
