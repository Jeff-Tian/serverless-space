{
  "name": "serverless-space",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "node index.js",
    "test": "jest",
    "build": "nest build&&cp src/babel-service/babel.min.js dist/src/babel-service/babel.min.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "offline": "yarn build && cross-env NODE_ENV=local serverless offline",
    "deploy": "serverless deploy --verbose&&(./smoke || sh smoke.sh)",
    "pre-deploy": "cd layers/nodejs && npm i --production --legacy-peer-deps&&cd ../../gatsby-plugins/nodejs&&(npm i --production --legacy-peer-deps && npm v gatsby dist.tarball | xargs curl | tar -xz&&(mv package node_modules/gatsby || echo 'mv skipped'))&& (cd ../../aws/nodejs && npm v aws-sdk dist.tarball | xargs curl | tar -xz&&(mv package node_modules/aws-sdk || echo 'mv skipped'))",
    "aws-console": "open https://console.aws.amazon.com/lambda/home?region=us-east-1#/functions",
    "aws-console:windows": "start https://console.aws.amazon.com/lambda/home?region=us-east-1#/functions",
    "studio:windows": "start https://jqp5j170i6.execute-api.us-east-1.amazonaws.com/dev/gatsby/graphql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jeff-Tian/serverless-space.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jeff-Tian/serverless-space/issues"
  },
  "homepage": "https://github.com/Jeff-Tian/serverless-space#readme",
  "dependencies": {
    "@apollo/gateway": "^2.1.4",
    "@apollo/subgraph": "^2.2.2",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
