    "xstate": "^4.38.2"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.18",
    "@nestjs/testing": "10.2.7",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.72",
    "@swc/jest": "^0.2.27",
    "@types/jest": "^29.5.3",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "gatsby-recipes": "1.4.0",
    "jest": "^29.6.2",
    "mocha": "^10.2.0",
    "nock": "^13.3.2",
    "rimraf": "^5.0.1",
    "serverless": "3.33.0",
    "serverless-lift": "^1.28.0",
    "serverless-offline": "12.0.4",
    "serverless-plugin-layer-manager": "1.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-mockery": "^1.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
