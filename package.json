    "resolve-from": "^5.0.0",
    "rxjs": "*",
    "serverless-express": "^2.0.11",
    "single-trailing-newline": "^1.0.0",
    "xstate": "^4.25.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/testing": "^8.0.6",
    "@types/jest": "^27.0.1",
    "gatsby-recipes": "0.9.3",
